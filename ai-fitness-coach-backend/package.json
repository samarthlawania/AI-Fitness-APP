{
  "name": "ai-fitness-coach-backend",
  "version": "1.0.0",
  "description": "Production-ready AI Fitness Coach backend with Express, PostgreSQL, and LLM integration",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "worker": "node src/workers/index.js",
    "local-dev": "docker-compose up -d && npm run dev",
    "build": "echo 'No build step needed for Node.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio"

  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.462.0",
    "@aws-sdk/s3-request-presigner": "^3.462.0",
    "@prisma/client": "^5.7.1",
    "aws-sdk": "^2.1506.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.15.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "pdfkit": "^0.14.0",
    "pino": "^8.16.2",
    "pino-http": "^8.5.1",
    "pino-pretty": "^10.2.3",
    "puppeteer": "^21.6.1",
    "redis": "^4.6.11",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
